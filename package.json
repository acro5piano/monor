{
  "name": "monor",
  "version": "0.1.11",
  "main": "src/index.js",
  "repository": "git@github.com:acro5piano/monor",
  "author": "gosho-kazuya <ketsume0211@gmail.com>",
  "license": "MIT",
  "bin": {
    "monor": "./bin/monor"
  },
  "scripts": {
    "format": "prettier --write src/*.{js,jsx,ts,tsx,md}",
    "lint": "tslint src/*.{ts,tsx}",
    "prepublish": "tsc",
    "example": "yarn prepublish && cd example && ../bin/monor",
    "test:unit": "alsatian src/*.test.ts",
    "test:e2e": "yarn prepublish && cd example && echo start:all | ../bin/monor"
  },
  "dependencies": {
    "alsatian": "^3.1.0",
    "bluebird": "^3.7.0",
    "enquirer": "^2.3.2",
    "glob": "^7.1.4",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/glob": "^7.1.1",
    "@types/node": "^12.11.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(core/vendor/assets/)*.{js,jsx,ts,tsx,md,css}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
